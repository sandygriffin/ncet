SELECT sv_ne.van_attempts.committeename, sv_ne.van_attempts.contacttypename, sv_ne.van_attempts.resultshortname,
COUNT (sv_ne.van_attempts.vanid) AS vanid 
FROM sv_ne.van_attempts 
WHERE (sv_ne.van_attempts.datecanvassed >= 2023)
GROUP BY sv_ne.van_attempts.committeename,sv_ne.van_attempts.contacttypename,sv_ne.van_attempts.resultshortname
ORDER BY sv_ne.van_attempts.committeename,sv_ne.van_attempts.contacttypename,sv_ne.van_attempts.resultshortname;
