SELECT
CASE
    WHEN sv_ne.catalist_person.age <= 26 THEN '18-25'
    WHEN sv_ne.catalist_person.age <= 42 THEN '26-41'
    WHEN sv_ne.catalist_person.age <= 58 THEN '42-57'
    WHEN sv_ne.catalist_person.age <= 77 THEN '58-77'
    ELSE '78+'
END AS Age_Range,
COUNT (sv_ne.van_activist_codes.vanid) AS vanid 
FROM sv_ne.van_attempts 
LEFT JOIN sv_ne.catalist_person
ON sv_ne.catalist_person.dwid = sv_ne.van_attempts.dwid
LEFT JOIN sv_ne.van_activist_codes
ON sv_ne.van_activist_codes.vanid = sv_ne.van_attempts.vanid
WHERE (sv_ne.van_activist_codes.activistcodename = '22 RTW Signer')
AND (sv_ne.catalist_person.age > 18)
AND (sv_ne.catalist_person.age IS NOT NULL)
GROUP BY Age_Range
ORDER BY Age_Range;
